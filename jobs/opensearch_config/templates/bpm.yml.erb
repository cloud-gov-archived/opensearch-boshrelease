processes:
  - name: opensearch_config
    executable: /var/vcap/jobs/opensearch_config/bin/job.process
    ephemeral_disk: true
    additional_volumes:
    - path: /var/vcap/jobs/opensearch-config-lfc
    - path: /var/vcap/sys/tmp/opensearch_config
      writable: true
      allow_executions: true
